#4.1 pizzas
pizzas = ["Pepperoni", "Hawaiian", "Meat Lovers"]
for pizza in pizzas:
    print(f"I like {pizza.title()} pizza.\n")

print("I really like pizza!")

#4.2 animals
animals = ["dog", "cat", "fox"]
for animal in animals:
    print(f"A {animal.title()} has four legs.\n")

print("All of these animals have four legs.")

#4.3 counting to twenty
for value in range(1, 21):
    print(value)
