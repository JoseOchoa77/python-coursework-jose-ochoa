rivers = {'nile': 'egypt', 'amazon': 'brazil', 'danube': 'germany'}


for river,country in rivers.items():
    print(f"The {river} river runs through {country}")

print("\nCountries:")
for river,country in rivers.items():
    print (country)

print("\nRivers:")
for river,country in rivers.items():
    print(river)

